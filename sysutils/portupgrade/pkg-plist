etc/rc.d/000.pkgtools.sh
@exec /bin/mkdir -p %D/lib/compat/pkg
@unexec /bin/rmdir %D/lib/compat/pkg 2>/dev/null || true
sbin/pkg_deinstall
sbin/pkg_fetch
sbin/pkg_glob
sbin/pkg_sort
sbin/pkg_which
sbin/pkgdb
sbin/portcvsweb
sbin/portinstall
sbin/ports_glob
sbin/portsclean
sbin/portsdb
sbin/portupgrade
sbin/portversion
