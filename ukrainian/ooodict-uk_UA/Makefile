COMMENT=	Ukrainian MySpell dictionary for OpenOffice.org
CATEGORIES=	ukrainian

LANGPREFIX=	uk
LANGSUFFIX=	UA
LANGDIST+=	uk_UA.zip
HYPHDIST+=	hyph_uk_UA.zip
MASTERDIR=	${.CURDIR}/../../editors/ooodict-all
RDEP=		${PORTSDIR}/editors/openoffice-1.1

.include "${MASTERDIR}/Makefile"
