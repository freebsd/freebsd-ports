	ftp -4Va ftp://ftp.freebsd.org/pub/FreeBSD/ports/$arch/packages-$sys-stable/All/ < /tmp/bpkg.tmp > /tmp/bpkg2.tmp

	n=1
	for e in `cat /tmp/bpkg1.tmp | awk '{print $9}' | cut -d. -f1`; do
