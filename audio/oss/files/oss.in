#!/bin/sh

# PROVIDE: oss
# REQUIRE: DAEMON
# KEYWORD: nojail shutdown

. /etc/rc.subr

name="oss"
rcvar=`set_rcvar`

stop_cmd="oss_stop"
start_cmd="oss_start"

load_rc_config $name

: ${oss_enable=NO}

oss_start()
{
	echo "Starting Open Sound System"
	%%PREFIX%%/sbin/soundon
}

oss_stop()
{
	echo "Stopping Open Sound System"
	%%PREFIX%%/sbin/soundoff
}

run_rc_command "$1"

