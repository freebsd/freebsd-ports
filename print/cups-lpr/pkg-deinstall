#!/bin/sh

REPLACES_BINARIES="/usr/bin/lpr
					/usr/bin/lp
					/usr/bin/lpq
					/usr/bin/lprm
					/usr/bin/lpstat
					/usr/sbin/lpc"

case $2 in
POST-DEINSTALL)
	for FILE in ${REPLACES_BINARIES}; do
		[ -L "${FILE}" ] && rm "${FILE}"
		[ -e "${FILE}.bak" ] && mv "${FILE}.bak" "${FILE}"
	done
;;
esac
exit 0
