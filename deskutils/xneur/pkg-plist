bin/xneur
@unexec if cmp -s %D/etc/xneur/xneurrc.sample %D/etc/xneur/xneurrc; then rm -f %D/etc/xneur/xneurrc; fi
etc/xneur/xneurrc.sample
@exec if [ ! -f %D/etc/xneur/xneurrc ] ; then cp -p %D/%F %B/xneurrc; fi
@dirrmtry etc/xneur
lib/xneur/libxneur.so.15
