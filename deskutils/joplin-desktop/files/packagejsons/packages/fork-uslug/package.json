    "jest": "29.7.0",
    "typescript": "5.8.3"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/jeremys/uslug.git"
  },
  "main": "./index",
  "exports": {
    ".": "./index.js",
    "./lib/uslug": {
      "types": "./lib/uslug.ts",
      "require": "./lib/uslug.js",
      "default": "./lib/uslug.js"
    }
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "bugs": {
