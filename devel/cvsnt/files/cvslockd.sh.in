#!/bin/sh
#
# $FreeBSD: /tmp/pcvs/ports/devel/cvsnt/files/Attic/cvslockd.sh.in,v 1.2 2006-02-20 20:46:54 dougb Exp $
#
# PROVIDE: cvslockd
# REQUIRE: LOGIN
# KEYWORD: shutdown
#
. %%RC_SUBR%%

[ -z "$cvslockd_enable" ] && cvslockd_enable="NO"

name=cvslockd
rcvar=`set_rcvar`
command=%%PREFIX%%/bin/${name}

load_rc_config ${name}
run_rc_command "$1"
