--- 1,20 ----
+ all: .pcf_done
+ 
+ .pcf_done:
+ 	  sh convert.sh
+ 	  touch .pcf_done
+ 
+ install: .pcf_done
+ 	( \
+ 	  if [ ! -d /usr/X11R6/lib/X11/fonts/non-cjk ] ; \
+ 	    then mkdir -p /usr/X11R6/lib/X11/fonts/non-cjk ; \
+ 	  fi ; \
+ 	)
+ 	cp *pcf* fonts.alias /usr/X11R6/lib/X11/fonts/non-cjk
+ 	( \
+ 	  cd /usr/X11R6/lib/X11/fonts/non-cjk ; \
+ 	  mkfontdir ; \
+ 	)
+ 
+ clean:
+ 	rm *pcf* .pcf_done
*** /dev/null	Thu Oct 31 02:01:27 1996
--- convert.sh	Thu Oct 31 02:33:48 1996
***************
*** 0 ****
--- 1,7 ----
+ #!/bin/sh
+ for i in *.bdf; \
+  do  \
+    j=`echo $i | sed -e 's/bdf/pcf/g'` ; \
+    bdftopcf $i > $j ; \
+    compress $j ; \
+  done
