bin/update-vc
@unexec if cmp -s %D/etc/videocache.conf %D/etc/videocache.conf.default; then rm -f %D/etc/videocache.conf; fi
etc/videocache.conf.default
@exec [ -f %B/videocache.conf ] || cp %B/%f %B/videocache.conf
