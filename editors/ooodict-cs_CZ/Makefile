COMMENT=	Czech (Czech Republic) MySpell dictionary for OpenOffice.org
CATEGORIES=	editors

LANGPREFIX=	cs 
LANGSUFFIX=	CZ
LANGDIST+=	cs_CZ.zip
HYPHDIST+=	hyph_cs.zip
MASTERDIR=	${.CURDIR}/../ooodict-all
RDEP=		${PORTSDIR}/editors/openoffice

.include "${MASTERDIR}/Makefile"
