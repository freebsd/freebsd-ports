@postexec echo "epkowa" >> %D/etc/sane.d/dll.conf
@postexec echo "usb /dev/uscanner0" > %D/etc/sane.d/epkowa.conf.sample
@preunexec sed -i "" -e '/^epkowa$/d' %D/etc/sane.d/dll.conf
@sample etc/sane.d/epkowa.conf.sample
