PORTNAME=	gtkportscan
PORTVERSION=	1.2
PORTREVISION=	1
CATEGORIES=	security
MASTER_SITES=	${MASTER_SITE_LOCAL}
MASTER_SITE_SUBDIR=	cpiazza

MAINTAINER=	ports@FreeBSD.org
COMMENT=	A simple port scanner that prints out open ports

WRKSRC=		${WRKDIR}/${PORTNAME}

USE_GNOME=	gtk12

.include <bsd.port.mk>
