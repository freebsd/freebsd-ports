-      timeout--;
-      continue;
-    }
-    rfbClientLog("TLS handshake failed: %s.\n", gnutls_strerror(ret));
-    FreeTLS(client);
-    return FALSE;
-  }
-
-  if (timeout <= 0)
-  {
-    rfbClientLog("TLS handshake timeout.\n");
-    FreeTLS(client);
-    return FALSE;
-  }
-
-  rfbClientLog("TLS handshake done.\n");
-  return TRUE;
