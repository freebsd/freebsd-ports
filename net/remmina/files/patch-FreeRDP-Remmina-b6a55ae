-{
-  rfbServerToClientMsg msg;
-
-  if (client->serverPort==-1)
-    client->vncRec->readTimestamp = TRUE;
-  if (!ReadFromRFBServer(client, (char *)&msg, 1))
-    return FALSE;
-
-  switch (msg.type) {
-
-  case rfbSetColourMapEntries:
-  {
-    /* TODO:
-    int i;
-    uint16_t rgb[3];
-    XColor xc;
-
