-            return FALSE;
-          break;
-        }
-        break;
-      }
-
-#ifdef LIBVNCSERVER_HAVE_LIBZ
-      case rfbEncodingZlib:
-      {
-	switch (client->format.bitsPerPixel) {
-	case 8:
-	  if (!HandleZlib8(client, rect.r.x,rect.r.y,rect.r.w,rect.r.h))
