-	    return FALSE;
-	  break;
-	}
-	break;
-     }
-
-#ifdef LIBVNCSERVER_HAVE_LIBJPEG
-      case rfbEncodingTight:
-      {
-	switch (client->format.bitsPerPixel) {
-	case 8:
