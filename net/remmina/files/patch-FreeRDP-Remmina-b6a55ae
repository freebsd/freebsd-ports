-    {
-      for (j = i % 16; j < 16; j++)
-	{
-	  rfbClientLog("   ");
-	  if ((j % 4) == 3) rfbClientLog(" ");
-	}
-      str[i % 16] = 0;
-      rfbClientLog("%s\n",str);
-    }
-
-  fflush(stderr);
-}
