-/* VeNCrypt sub auth. 1 byte auth count, followed by count * 4 byte integers */
-static rfbBool
-ReadVeNCryptSecurityType(rfbClient* client, uint32_t *result)
-{
-    uint8_t count=0;
-    uint8_t loop=0;
-    uint8_t flag=0;
-    uint32_t tAuth[256], t;
