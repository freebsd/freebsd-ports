etc/rc.d/radiusd.sh
@dirrm etc/raddb
@dirrm share/doc/radiusd-cistron
@unexec rm -f /var/log/radius.log
@unexec rm -f /var/run/radiusd.pid
@unexec echo "Leaving /var/run/radutmp, /var/log/radwtmp and /var/log/radacct alone."
