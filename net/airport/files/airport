#! /bin/sh

case $0 in
	*airport)
		file=AirportBaseStationConfig
	;;
	*airport2)
		file=Airport2BaseStationConfig
	;;
	*hostmon)
		file=HostMonitor
	;;
	*linkmon)
		file=LinkMonitor
	;;
	*airmodem)
		file=AirportModemUtility
	;;
	*)
	echo "Unknown command"
	exit 1
esac

exec %%PREFIX%%/bin/javavm -jar %%PREFIX%%/share/java/${file}.jar

exit 1
