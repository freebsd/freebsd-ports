-                                numKeysUpdated++;
-                                me.updateCount = updateCount;
-                            }
-                        } else {
-                            // ready ops have already been set on this update
-                            ski.channel.translateAndUpdateReadyOps(rOps, ski);
-                        }
-                    } else {
