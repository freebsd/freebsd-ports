-                            numKeysUpdated++;
-                            me.updateCount = updateCount;
-                        }
-                    }
-                }
-            }
-        }
-
-        if (interrupted) {
-            // Clear the wakeup pipe
-            synchronized (interruptLock) {
-                IOUtil.drain(fd0);
-                interruptTriggered = false;
-            }
-        }
-        return numKeysUpdated;
-    }
