bin/ggz-txt
lib/ggz/tttxt
share/applications/ggz-txt.desktop
@unexec ggz-config -D --remove --modfile=%D/share/ggz/tttxt.dsc --force
share/ggz/tttxt.dsc
@exec ggz-config -D --install --modfile=%D/share/ggz/tttxt.dsc --force
%%NLS%%share/locale/de/LC_MESSAGES/ggz-txt.mo
@dirrmtry share/ggz
@dirrmtry lib/ggz
