+
+/* Define TEST to make a linkable ROM dumper */
+#ifdef TEST
+#define SOCKNUM 1
+#define NUMSOCKS 2
+
+#include <sys/types.h>
+#include <sys/stat.h>
+#include <fcntl.h>
